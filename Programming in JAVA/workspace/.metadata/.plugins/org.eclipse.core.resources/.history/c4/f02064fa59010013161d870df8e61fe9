
import urllib
import urllib2


req = urllib2.Request(
    url='http://v.163.com/special/opencourse/innovation.html',
)

result = urllib2.urlopen(req).read()
outfile = open("result.html", "w");
outfile.write(result);

from pyquery import PyQuery as pq
from lxml import etree

print dom = pq(result).text()

statusTbl = pq(dom('#list2')).find('tbody')
print statusTbl
